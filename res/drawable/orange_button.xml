<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#f99d3b" />
      <stroke
          android:width="1dp"
          android:color="#f99d3b" />
      <corners
          android:radius="0dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#f68002"
          android:endColor="#f68002"
          android:angle="270" />
      <stroke
          android:width="0dp"
          android:color="#f68002" />
      <corners
          android:radius="0dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>